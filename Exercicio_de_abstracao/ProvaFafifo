package N2;

public class ProvaFafifo extends ProvaUniversidade{
    public ProvaFafifo(double AV1, double AV2) {
        super(AV1, AV2);
    }

    public boolean aprovado(){
        boolean status;
        double media = calcularMedia();;
        if(media >= 6){
            status = true;
        }

        else{
            status = false;
        }

        return status;
    }
}
